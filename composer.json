{
    "name": "webvijayi/instant-checkout-via-acp-agentic-commerce-for-woocommerce",
    "description": "Instant Checkout via ACP Agentic Commerce - WooCommerce integration for the Agentic Commerce Protocol (ACP) enabling 'Buy it in ChatGPT' functionality",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Web Vijayi",
            "email": "webvijayi.com@gmail.com",
            "homepage": "https://webvijayi.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "stripe/stripe-php": "^18.0",
        "justinrainbow/json-schema": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "WebVijayi\\InstantCheckoutACP\\": "includes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        },
        "cafile": "C:/Projects/Woo-ACP/acp-instant-checkout-for-woocommerce/cacert.pem",
        "disable-tls": false,
        "secure-http": false,
        "capath": "C:/Projects/Woo-ACP/acp-instant-checkout-for-woocommerce"
    },
    "scripts": {
        "build": [
            "@composer install --no-dev --optimize-autoloader"
        ]
    }
}
